    <header>
        <nav class="navbar navbar-expand-md" style="background-color: #F5EED1; font-size: 18px;">
    <div class="navbar-collapse collapse w-100 order-1 order-md-0 dual-collapse2">
        &nbsp;&nbsp;
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
           <img src="~/images/IHP_logo.png" class="mr-3" alt="IHP Logo" width="100" />
        </a>
        <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Client" asp-action="Index">Client</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="HealthCompany" asp-action="Index">Health</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="InsuranceCompany" asp-action="Index">Insurance</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Lawyer" asp-action="Index">Lawyer</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Appointment" asp-action="Index">Appointment</a>
            </li>
        </ul>
    </div>

    </nav>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
       
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - InteractHealthPro Database - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>   
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
